{
  "compilerOptions": {
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./"
  },
  "include": ["**/*.ts"],
  "exclude": ["**/*.js", "**/*.js.map", "**/*.d.ts", "**/*.d.ts.map"]
}
